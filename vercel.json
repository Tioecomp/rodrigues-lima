{
    "version": 2,
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "css/*.css",
            "use": "@vercel/static"
        },
        {
            "src": "js/*.js",
            "use": "@vercel/static"
        },
        {
            "src": "assets/**/*",
            "use": "@vercel/static"
        },
        {
            "src": "public/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        }
    ]
}